{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@jarmee/kafka/(.*)": "<rootDir>/../libs/kafka/src/$1",
    "@jarmee/kafka": "<rootDir>/../libs/kafka/src"
  }
}